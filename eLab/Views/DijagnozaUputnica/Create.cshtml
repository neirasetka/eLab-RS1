@model eLab.Models.DijagnozaUputnica

@{
    ViewData["Title"] = "Create";
}

<h1>Dodaj</h1>

<h4>DijagnozaUputnica</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="DijagnozaId" class="control-label"></label>
                <select asp-for="DijagnozaId" class ="form-control" asp-items="ViewBag.DijagnozaId"></select>
            </div>
            <div class="form-group">
                <label asp-for="UputnicaId" class="control-label"></label>
                <select asp-for="UputnicaId" class ="form-control" asp-items="ViewBag.UputnicaId"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Nazad</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
